@model IEnumerable<Clinic_Management_System.Feedback>

@{
    ViewBag.Title = "Feedback_List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Feedback_List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Complain)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Suggestion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Satisfaction)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.User_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Complain)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Suggestion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Satisfaction)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
